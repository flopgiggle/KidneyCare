.container{
    display: flex;
    flex-direction:column;
    align-items: center;
    background-color:#ffffff;
}

.avatar{
    width:200rpx;
    height:200rpx;
    margin-top:160rpx;
}

.motto{
    margin-top:100rpx;
    font-size:32rpx;
    font-weight: bold;
}

.journey-container{
    margin-top: 50rpx;
    border: 2px solid #4fc1e9;
    width: 200rpx;
    height: 80rpx;
    border-radius: 3px;
    text-align:center;
    background-color:#4fc1e9;
}

.journey{
    font-size:35rpx;
    font-weight: bold;
    line-height:80rpx;
    color: #ffffff;
}

page{
    height: 100%;
    background-color:#4fc1e9;
}